<%=form_with(model: @item, builder: CustomFormBuilder) do |f|%>
  <%=f.label("Item Name:") %>
  <%=f.text_field(:name) %>
  <%=f.label("Measurements:") %>
  <%=f.my_fields_for(:measurements) do |m| %>
    <%=m.label "Quantity"%>
    <%=m.number_field :quantity%>
    <%=m.label "Units"%>
    <%=m.text_field :unit%>
  <%end%>
  <%=f.submit "Submit"%>
<%end%>
